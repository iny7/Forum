<%= form_for @user do |f| %>
  <%= f.label :name, 'username'%>
  <%= f.text_field :name %>
  
  <%= f.label :email, 'email'%>
  <%= f.text_field :email %>
  
  <%= f.label :password, 'password'%>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation, 'comfirm password'%>
  <%= f.password_field :password_confirmation %>
  
  <%= f.submit 'Sign Up' %>
<% end %>
